{
  "name": "my-blog",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "migrate": "node migrate-wordpress.js",
    "migrate:sample": "node migrate-simple.js",
    "seo:sitemap": "node scripts/generate-complete-sitemap.js",
    "seo:submit": "node scripts/submit-to-search-engines.js",
    "seo:check": "node scripts/seo-health-check.js",
    "seo:online": "node scripts/check-online-status.js",
    "seo:all": "npm run seo:sitemap && npm run seo:online && npm run seo:submit",
    "deploy:simple": "node scripts/simple-deploy.js",
    "deploy:test-server": "node -e \"import('./scripts/modules/server-sync.js').then(m => m.testServerConnection())\"",
    "deploy:test-cdn": "node -e \"import('./scripts/modules/cdn-sync.js').then(m => m.testCDNConnection())\"",
    "deploy:sitemap": "node -e \"import('./scripts/modules/sitemap-generator.js').then(m => m.generateSitemap())\""
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.2",
    "@astrojs/node": "^9.3.3",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.2",
    "@astrojs/tailwind": "^6.0.2",
    "@supabase/supabase-js": "^2.53.0",
    "astro": "^5.12.7",
    "jsdom": "^26.1.0",
    "sharp": "^0.34.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "cos-nodejs-sdk-v5": "^2.11.19",
    "node-fetch": "^3.3.2",
    "turndown": "^7.2.0"
  }
}
